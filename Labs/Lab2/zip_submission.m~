% @file zip_submission.m
% @brief Zips submissions.
%
% @author Oscar Bezi (bezi@cmu.edu)
% @since 19 January 2016
%--------------------------------------------------------------------------t
function [] = zip_submission(andrewID)
zipFile = sprintf('%s_lab2.zip', andrewID);
zip(zipFile, {'student_images', ...
    'ta_images/l1/images.dat', 'ta_images/l2/images.dat', 'ta_images/l3/images.dat', 'ta_images/l4/images.dat' ...
    'calculate_centroids.m', ...
    'Blob.m'
    'centroids_to_dist.m', ...
    'image_to_dist.m', ...
    'segment_image.m', ...
    'threshold_image.m'})
end